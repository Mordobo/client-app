{
  "expo": {
    "name": "Mordobo",
    "slug": "mordobo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mordobo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.mordobo.client",
      "supportsTablet": true
    },
    "android": {
      "package": "com.mordobo.client",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.756021799555-6bv6qnjcdkchf9i7luomkucoui849cd4"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "googleWebClientId": "756021799555-c1mb6ff3sg8gus851t1bl3ef76mupd34.apps.googleusercontent.com",
      "googleIosClientId": "756021799555-6bv6qnjcdkchf9i7luomkucoui849cd4.apps.googleusercontent.com",
      "googleAndroidClientId": "756021799555-oj6pnq9f50l1m0pb8rjk5rtakfgojdil.apps.googleusercontent.com",
      "googleIosUrlScheme": "com.googleusercontent.apps.756021799555-6bv6qnjcdkchf9i7luomkucoui849cd4",
      "expoPublicApiUrl": "http://localhost:3000"
    }
  }
}
